<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.homework.web.taobao.dao.UserDao">
    <!-- 根据主键查询-->
    <select id="getById" resultType="com.homework.web.taobao.entity.UserEntity" parameterType="java.lang.String" >
        select  *
        from user
        where id = #{id}
    </select>
    <!-- 用户注册 -->
    <insert id="userInsert" parameterType="com.homework.web.taobao.entity.UserEntity" useGeneratedKeys="true" keyProperty="uid">
    insert into user (name, id)
    values (#{name,jdbcType=VARCHAR}, #{id,jdbcType=VARCHAR})
  </insert>

</mapper>